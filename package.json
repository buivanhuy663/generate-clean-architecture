{
  "name": "generate-clean-architecture",
  "displayName": "generator_page",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clean-architecture.create-new-page",
        "title": "New Page"
      },
      {
        "command": "clean-architecture.create-new-use-case",
        "title": "New Use Case"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "create-new-page",
          "group": "@clean-architecture"
        }
      ],
      "create-new-page": [
        {
          "command": "clean-architecture.create-new-page",
          "group": "@clean-architecture"
        },
        {
          "command": "clean-architecture.create-new-use-case",
          "group": "@clean-architecture"
        }
      ]
    },
    "submenus": [
      {
        "label": "Clean Architecture",
        "id": "create-new-page"
      }
    ]
  },
  "activationEvents": [
    "onCommand:extension.create-new-page",
    "workspaceContains:**/pubspec.yaml"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.191",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vscode/test-electron": "^2.2.1",
    "eslint": "^8.30.0",
    "glob": "^8.0.3",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "node-fetch": "^3.3.0",
    "semver": "^7.3.8"
  }
}